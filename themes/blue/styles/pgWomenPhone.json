{
  "#pgWomenPhone": {
    "&-flMain": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "COLUMN",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START"
      },
      "+page:Screen.width > Screen.height": {
        "flexProps": {
          "flexDirection": "ROW"
        }
      },
      "+isTablet_portrait:": {
        "flexProps": {
          "flexDirection": "COLUMN"
        }
      },
      "+isTablet_landscape:": {
        "flexProps": {
          "flexDirection": "ROW"
        }
      }
    },
    "&-flButtons": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "ROW",
        "alignItems": "CENTER",
        "justifyContent": "SPACE_BETWEEN"
      },
      "+page:(Screen.width > 450  && Screen.width < 800) || (Screen.width > 800 && Screen.width > Screen.height)": {
          "flexProps": {
            "justifyContent": "FLEX_START"
          }
      },
      "marginTop": 10,
      "marginBottom": 10,
      "+isTablet_portrait:": {
        "flexProps": {
          "flexGrow": 1,
          "alignItems": "CENTER",
          "justifyContent": "CENTER",
          "alignContent": "CENTER"
        }
      },
      "+isTablet_landscape:": {
        "flexProps": {
          "flexGrow": 1.2,
          "flexDirection": "COLUMN",
          "alignItems": "STRETCH",
          "alignContent": "STRETCH"
        }
      }
    },
    ".flInfo": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 2.8,
        "justifyContent": "SPACE_BETWEEN",
        "flexDirection": "COLUMN",
        "alignItems": "STRETCH",
        "alignContent": "STRETCH",
        "alignSelf": "STRETCH"
      },
      "marginBottom": null,
      "+page:(Screen.width > 450  && Screen.width < 800) || (Screen.width > 800 && Screen.width > Screen.height)": {
        "marginBottom": 20
      },
      "+isTablet_portrait:": {
        "flexProps": {
          "flexProps": 1.5
        }
      },
      "&-flPart1": {
        "&-lblProductName": {
          "width": null,
          "font": {
            "size": 18,
            "family": "Lato"
          },
          "+isTablet:": {
            "font": {
              "size": 38,
              "family": "Lato"
            }
          }
        },
        "&-flDetail": {
          "height": null,
          "width": null,
          "flexProps": {
            "flexGrow": 1,
            "alignItems": "STRETCH",
            "alignSelf": "STRETCH",
            "flexDirection": "COLUMN"
          },
          "+page:(Screen.width > 450  && Screen.width < 800) || (Screen.width > 800 && Screen.width > Screen.height)": {
            "flexProps": {
              "flexDirection": "ROW"
            }
          },
          "&-lblProductPrice": {
            "width": null,
            "font": {
              "size": 18,
              "family": "Lato"
            },
            "+isTablet:": {
              "font": {
                "size": 38,
                "family": "Lato"
              }
            }
          },
          "&-lblProductDetail": {
            "width": null,
            "flexProps": {
              "flexGrow": 1
            },
            "font": {
              "size": 12,
              "family": "Lato"
            },
            "+isTablet:": {
              "font": {
                "size": 30,
                "family": "Lato"
              },
              "flexProps": {
                "flexGrow": 1.7
              }
            }
          }

        }
      },
      "&-flPart2": {
        "width": null,
        "height": null,
        "flexProps": {
          "flexGrow": 2,
          "flexDirection": "COLUMN"
        },
        "+page:(  Screen.width > 450  && Screen.width < 800) || (Screen.width > 800 && Screen.width > Screen.height)": {
          "flexProps": {
            "flexGrow": 4,
            "flexDirection": "COLUMN_REVERSE"
          }
        },
        "+isTablet_portrait:": {
          "flexProps": {
            "flexGrow": 3,
            "flexDirection": "COLUMN"
          }
        },
        "&-flInfo": {
          "&-lbl": {
            "width": null,
            "height": null,
            "flexProps": {
              "flexGrow": 1
            },
            "font": {
              "size": 12,
              "family": "Lato"
            },
            "+isTablet:": {
              "font": {
                "size": 30,
                "family": "Lato"
              }
            }
          }
        }
      }
    },
    "&-btnAddToCard": {
      "marginBottom": null,
      "marginTop": null,
      "marginRight": null,
      "width": null,
      "height": 50,
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "STRETCH"
      },
      "font": {
        "family": "Lato",
        "size": 15
      },
      "backgroundColor": "rgba(210,49,130,1)",
      "marginRight": 10,
      "+isTablet:": {
        "height": 70,
        "font": {
          "family": "Lato",
          "size": 32
        },
        "marginRight": null
      },
      "+isTablet_portrait:": {
        "marginRight": 10
      },
      "+isTablet_landscape:": {
        "marginBottom": 10
      }
    },
    "&-btnQuickOrder": {
      "marginBottom": null,
      "marginTop": null,
      "marginRight": null,
      "width": null,
      "height": 50,
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "STRETCH"
      },
      "font": {
        "family": "Lato",
        "size": 15
      },
      "backgroundColor": "rgba(103,164,231,1)",
      "marginLeft": null,
      "+isTablet:": {
        "height": 70,
        "font": {
          "family": "Lato",
          "size": 32
        }
      },
      "+isTablet_portrait:": {
        "marginLeft": 10
      },
      "+isTablet_landscape:": {
        "marginTop": 10
      }
    },
    ".btnColor": {

      "width": 15,
      "height": 15,
      "marginLeft": 11,
      "+isTablet:": {
        "width": 35,
        "height": 35
      }
    },
    ".flSwipe": {
      "maxWidth": null,
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1
      },
      "paddingLeft": 20,
      "paddingRight": 20,
      "+page:Screen.width > 450": {
        "maxHeight": null,
        "paddingLeft": 5,
        "paddingRight": 5
      },
      "&-dotIndicator": {
        "width": null,
        "height": 10,
        "top": null,
        "left": 0,
        "right": 0,
        "bottom": 8,
        "flexProps": {
          "positionType": "ABSOLUTE",
          "justifyContent": "CENTER",
          "flexDirection": "ROW",
          "alignItems": "CENTER"
        }
      }
    },
    ".flInfoOuter": {
      "marginLeft": 20,
      "marginRight": 20,
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "COLUMN"
      },
      "marginTop": null,
      "+page:(Screen.width > 450  && Screen.width < 800) || (Screen.width > 800 && Screen.width > Screen.height)": {
        "flexProps": {
          "flexProps": 1.4
        },
        "marginTop": 5
      }
    },
    "&_lineUnderColor": {
      "maxHeight": 1,
      "maxWidth": null,
      "width": null,
      "height": 1,
      "alpha": 0.1,
      "visible": true,
      "flexProps": {
        "flexGrow": 1
      },
      "marginTop": null,
      "+isTablet:": {
        "marginTop": 15
      },
      "backgroundColor": "#1D1D26"
    }
  }
}
