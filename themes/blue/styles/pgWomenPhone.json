{
  "#pgWomenPhone": {
    ".flButtons": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "ROW",
        "alignItems": "CENTER",
        "justifyContent": "SPACE_BETWEEN"
      },
      "+page:Screen.width > 450": {
        "flexProps": {
          "justifyContent": "FLEX_START"
        }
      }
    },
    ".flInfo": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 2,
        "justifyContent": "SPACE_BETWEEN",
        "flexDirection": "COLUMN",
        "alignItems": "STRETCH",
        "alignContent": "STRETCH",
        "alignSelf": "STRETCH"
      }
    },
    ".flPart2": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 2,
        "flexDirection": "COLUMN"
      },
      "+page:Screen.width > 450": {
        "flexProps": {
          "flexGrow": 4,
          "flexDirection": "COLUMN_REVERSE"
        }
      }
    },
    "&-flDetail": {
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "COLUMN"
      },
      "+page:Screen.width   >   449 ": {
        "flexProps": {
          "flexDirection": "ROW"
        }
      }
    },
    ".page": {
      "flexProps": {
        "flexDirection": "COLUMN",
        "alignItems": "STRETCH",
        "alignContent": "STRETCH"
      },
      "+page:Screen.width > 448": {
        "flexProps": {
          "flexDirection": "ROW"
        }
      }
    },
    ".btnAddToCard": {
      "width": null,
      "height": 50,
      "flexProps": {
        "flexGrow": 1
      },
      "font": {
        "family": "Lato",
        "size": 15
      },
      "backgroundColor": "rgba(210,49,132,1)",
      "marginRight": 5,
      "+page:Screen.width > 450": {
        "marginRight": 40,
        "marginTop": 10,
        "marginBottom": 5
      }
    },
    ".btnQuickOrder": {
      "width": null,
      "height": 50,
      "flexProps": {
        "flexGrow": 1
      },
      "font": {
        "family": "Lato",
        "size": 15
      },
      "backgroundColor": "rgba(103,164,231,1)",
      "marginLeft": null,
      "+page:Screen.width > 450": {
        "marginTop": 5,
        "marginLeft": 10,
        "marginBottom": 10
      }
    },
    ".btnColor": {
      "width": 15,
      "height": 15,
      "marginLeft": 11
    },
    ".flSwipe": {
      "maxWidth": null,
      "height": null,
      "width": null,
      "maxHeight": 359,
      "flexProps": {
        "flexGrow": 1
      },
      "paddingLeft": 20,
      "paddingRight": 20,
      "+page:Screen.width > 450": {
        "maxWidth": 225,
        "maxHeight": null,
        "flexProps": {},
        "paddingLeft": 5,
        "paddingRight": 5
      }
    },
    ".flInfoOuter": {
      "marginLeft": 20,
      "marginRight": 20,
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1,
        "flexDirection": "COLUMN"
      },
      "marginTop": null,
      "+page:Screen.width > 450": {
        "flexProps": {
          "flexProps": 1.4
        },
        "marginTop": 5
      }
    },
    "&_lineUnderColor": {
      "maxHeight": 1,
      "maxWidth": null,
      "width": null,
			"alpha": 0.1,
			"visible": true,
			"backgroundColor": "#1D1D26",
      "width": null,
      "+page:Screen.width > 450": {
        "maxHeight": 0,
        "maxWidth": 0,
        "visible": false
      }
    }
  }
}
