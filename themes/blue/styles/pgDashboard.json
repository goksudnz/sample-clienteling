    ".infoPart2": {
      "height": null,
      "width": null,
      "paddingRight": 20,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "ROW",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+page:Screen.width > 450": {
        "flexDirection": "COLUMN"
      }
    }
  }
