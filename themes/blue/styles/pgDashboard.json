{
  "#pgDashboard": {
    ".flEmployeeInfo": {
      "height": 200,
      "width": null,
      "align": "HORIZONTAL",
      "flexProps": {
        "flexDirection": "COLUMN",
        "flexWrap": "WRAP",
        "positionType": "RELATIVE"
      },
      "+page:Screen.width > 450": {
        "flexProps": {
          "flexDirection": "ROW"
        },
        "height": 100
      }
    },
    ".svMain": {
      "height": null,
      "width": null,
      "visible": true,
      "align": "VERTICAL",
      "scrollBarEnabled": true,
      "layoutHeight": 1250,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "COLUMN",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+page:Screen.width > 1000 || (Screen.width > 450 && Screen.width < Screen.height)": {
        "layoutHeight": 750
      }
    },
    ".flItem": {
      "height": 200,
      "width": null,
      "touchEnabled": true,
      "visible": true,
      "backgroundColor": "rgba( 184, 233, 134, 0 )",
      "alpha": 1,
      "borderColor": "rgba(0,0,0,1)",
      "borderWidth": 0,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "COLUMN",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "paddingBottom": null,
      "marginRight": 15.68,
      "marginBottom": 20.5,
      "minWidth": 240,
      "maxWidth": 600,
      "+page:Screen.width > 450": {
        "minWidth": 400
      },
      ".small": {
        "height": 119.25,
        "+page:Screen.width > 450": {
          "minWidth": 400
        }
      }
    },
    ".infoPart1": {
      "height": null,
      "width": null,
      "paddingRight": 20,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "ROW",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+page:Screen.width > 450": {
        "flexDirection": "COLUMN"
      }
