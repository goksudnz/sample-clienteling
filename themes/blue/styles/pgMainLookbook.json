{
  "#pgMainLookbook": {
    ".lblSale": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1
      },
      "font": {
        "size": 22
      },
      "textAlignment": "MIDCENTER"
    },
    ".lbl50Off": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1
      },
      "textColor": "#D52682",
      "multiline": true,
      "textAlignment": "MIDCENTER",
      "font": {
        "size": 25
      },
      "+page:Screen.width > 450": {
        "multiline": false
      }
    },
    ".btnShopNow": {
      "width": 100,
      "height": 30,
      "borderColor": "#000000",
      "borderWidth": 2,
      "backgroundColor": "rgba(0,0,0,0)",
      "textColor": "#000000"
    },
    ".flShopNow": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1,
        "alignItems": "CENTER",
        "justifyContent": "CENTER"
      }
    },
    ".flFirstLine": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 39,
        "flexDirection": "ROW"
      }
    },
    ".flSecondLine": {
      "width": null,
      "height": null,
      "maxHeight": null,
      "maxWidth": null,
      "flexProps": {
        "flexGrow": 27
      },
      "+page:Screen.width > 450 && Screen.width > Screen.height": {
        "width": 0,
        "height": 0,
        "maxHeight": 0,
        "maxWidth": 0,
        "flexProps": {
          "flexGrow": 0
        }
      }
    },
    ".flThirdLine": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 65,
        "flexDirection": "ROW"
      }
    },
    ".flSale": {
      "width": 0,
      "height": 0,
      "maxHeight": 0,
      "maxWidth": 0,
      "flexProps": {
        "flexGrow": 0
      },
      "+page:Screen.width > 450 && Screen.width > Screen.height": {
        "width": null,
        "height": null,
        "maxHeight": null,
        "maxWidth": null,
        "flexProps": {
          "flexGrow": 2.8,
          "flexDirection": "COLUMN"
        }
      }
    },
    ".lblSaleBackground": {
      "left": 0,
      "right": 0,
      "top": 0,
      "bottom": 0,
      "width": null,
      "height": null,
      "textAlignment": "MIDCENTER",
      "textColor": "rgba(29,29,38,0.1)",
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "font": {
        "size": 100,
        "bold": true
      },
      "+page:Screen.width > 450": {
        "font": {
          "size": 70
        },
        "textAlignment": "TOPCENTER"
      }
    },
    ".lblGender": {
      "width": null,
      "height": 40,
      "textAlignment": "MIDRIGHT",
      "textColor": "#FFFFFF",
      "font": {
        "size": 30
      },
      "marginRight": 10,
      "marginLeft": null,
      ".men": {
        "textAlignment": "MIDLEFT",
        "marginLeft": 10,
        "+page:Screen.width > 1000": {
          "height": 70,
          "font": {
            "size": 42
          }
        }
      },
      "+page:Screen.width > 1000": {
        "height": 70,
        "font": {
          "size": 42
        }
      }
    },
    ".lblShop": {
      "width": null,
      "height": 20,
      "textAlignment": "MIDRIGHT",
      "marginRight": 10,
      "marginLeft": null,
      "font": {
        "size": 15
      },
      ".men": {
        "textAlignment": "MIDLEFT",
        "marginLeft": 10,
        "+page:Screen.width > 1000": {
          "height": 50,
          "font": {
            "size": 26
          }
        }
      },
      "+page:Screen.width > 1000": {
        "height": 50,
        "font": {
          "size": 26
        }
      }
    },
    ".lblSummer": {
      "width": null,
      "height": 30,
      "textAlignment": "MIDLEFT",
      "multiline": true,
      "marginLeft": 20,
      "marginRight": null,
      "+page:Screen.width > 450 ": {
        "multiline": false,
        "textAlignment": "MIDRIGHT",
        "marginRight": 30
      },
      "+page:Screen.width > 1000 ": {
        "multiline": false,
        "textAlignment": "MIDRIGHT",
        "marginRight": 30,
        "height": 50,
        "font": {
          "size": 18
        }
      }
    },
    ".lblBag": {
      "width": null,
      "height": 60,
      "textAlignment": "MIDCENTER",
      "multiline": true,
      "textColor": "#FFFFFF",
      "font": {
        "size": 20
      },
      "marginRight": null,
      "+page:Screen.width > 450": {
        "textAlignment": "MIDRIGHT",
        "marginRight": 30
      },
      "+page:Screen.width > 1000": {
        "height": 80,
        "font": {
          "size": 36
        }
      }
    },
    ".lblAccessories": {
      "width": null,
      "height": 20,
      "textAlignment": "MIDCENTER",
      "multiline": true,
      "+page:Screen.width > 1000": {
        "height": 40,
        "font": {
          "size": 18
        }
      }
    },
    ".lblGlasses": {
      "width": null,
      "textAlignment": "MIDCENTER",
      "height": 30,
      "textColor": "#FFFFFF",
      "font": {
        "size": 20
      },
      "+page:Screen.width > 450": {
        "height": 20
      },
      "+page:Screen.width > 1000": {
        "height": 70,
        "font": {
          "size": 36
        }
      }
    },
    ".imgWomen": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1
      },
      "imageFillType": "STRETCH",
      "image": "pgMainLookbook_women_vertical.png",
      "+page:Screen.width > 450": {
        "image": "pgMainLookbook_women_horizontal.png"
      }
    },
    ".imgMen": {
      "width": null,
      "height": null,
      "flexProps": {
        "flexGrow": 1
      },
      "imageFillType": "STRETCH",
      "image": "pgMainLookbook_men_vertical.png",
      "+page:Screen.width > 450": {
        "image": "pgMainLookbook_men_horizontal.png"
      }
    }
  }
}
