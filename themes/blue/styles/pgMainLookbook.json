			"width": null,
			"height": null,
			"flexProps": {
				"flexGrow": 1
			},
			"imageFillType": "STRETCH",
			"image": "pgmainlookbook_men_vertical.png",
			"+page:Screen.width > 450": {
				"image": "pgmainlookbook_men_horizontal.png"
			}
		},
		"&-imgBagTrends": {
			"flexProps": {
				"flexGrow": 1,
				"positionType": "RELATIVE"
			},
			"height": null,
			"image": "bag.png",
			"imageFillType": "ASPECTFIT",
			"marginLeft": -50,
