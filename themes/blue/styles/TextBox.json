{
  ".textBox": {
    "height": 70,
    "width": 200,
    "touchEnabled": true,
    "text": "TextBox",
    "visible": true,
    "backgroundColor": "#FFFFFF",
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "textColor": "#1D1D26",
    "textAlignment": "MIDLEFT",
    "font": {
      "size": 14,
      "bold": false,
      "italic": false,
      "family": "Lato"
    },
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    }
  }
}
