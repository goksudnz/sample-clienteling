{
  ".label": {
    "width": 250,
    "height": 70,
    "touchEnabled": true,
    "visible": true,
    "multiline": false,
    "backgroundColor": "rgba(255,255,255,0)",
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "textColor": "#1D1D26",
    "textAlignment": "MIDLEFT",
    "font": {
      "size": 10,
      "bold": false,
      "italic": false,
      "family": "Lato"
    },
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-header": {
      "textColor": "rgba(162,162,162,1)",
      "font": {
        "size": 14
      },
      "width": null,
      "height": 20,
      ".shopping": {
        "height": null,
        "textAlignment": "BOTTOMCENTER",
        "marginBottom": 5,
        "flexProps": {
          "flexGrow": 1
        },
        "font": {
          "size": 20
        }
      }
    }
  }
  
}
