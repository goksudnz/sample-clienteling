{
  "#pgSignupTablet": {
    ".btnFacebook": {
      "width": null,
      "maxHeight": 70,
      "marginLeft": 20,
      "marginRight": 10,
      "marginBottom": null,
      "marginTop": null,
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "STRETCH"
      },
      "font": {
        "size": 28,
        "family": "Lato"
      },
      "backgroundColor": "rgba(71,94,174,1)",
      "+isTablet_landscape:": {
        "marginRight": 20,
        "marginBottom": 10,
        "marginTop": 20
      }
    },
    ".btnAnonymous": {
      "width": null,
      "maxHeight": 70,
      "marginLeft": 10,
      "marginRight": 20,
      "marginTop": null,
      "flexProps": {
        "flexGrow": 1,
        "alignSelf": "STRETCH"
      },
      "font": {
        "size": 28,
        "family": "Lato"
      },
      "backgroundColor": "rgba(103,164,231,1)",
      "textColor": "#FFFFFF",
      "borderWidth": 0,
      "+isTablet_landscape:": {
        "marginLeft": 20,
        "marginTop": 10,
        "backgroundColor": "#FFFFFF",
        "textColor": "rgba(103,164,231,1)",
        "borderColor": "#979393",
        "borderWidth": 1
      }
    },
    ".btnSignup": {
      "backgroundColor": "rgba(210,49,132,1)",
      "width": null,
      "maxHeight": 70,
      "marginLeft": 20,
      "marginRight": 20,
      "flexProps": {
        "flexGrow": 1
      },
      "textAlignment": "MIDCENTER",
      "font": {
        "size": 28,
        "family": "Lato"
      }
    },
    ".flOther": {
      "height": null,
      "width": null,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "ROW",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+isTablet_landscape:": {
        "flexProps": {
          "flexGrow": 2.5,
          "flexDirection": "COLUMN",
          "alignItems": "FLEX_START",
          "justifyContent": "FLEX_START",
          "alignContent": "STRETCH"
        }
      }
    },
    ".flMain": {
      "height": null,
      "width": null,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "COLUMN",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+isTablet_landscape:": {
        "flexProps": {
          "flexDirection": "ROW"
        }
      }
    },
    ".flInfo": {
      "height": null,
      "width": null,
      "marginBottom": null,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "STRETCH",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "COLUMN",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+isTablet_landscape:": {
        "marginBottom": 20
      }
    },
    ".flSignup": {
      "height": null,
      "width": null,
      "flexProps": {
        "alignSelf": "AUTO",
        "direction": "INHERIT",
        "alignContent": "STRETCH",
        "alignItems": "CENTER",
        "justifyContent": "FLEX_START",
        "flexWrap": "NOWRAP",
        "flexDirection": "ROW",
        "positionType": "RELATIVE",
        "flexGrow": 1
      },
      "+isTablet_landscape:": {
        "flexProps": {
          "alignItems": "FLEX_END"
        }
      }
    }
  }
}
