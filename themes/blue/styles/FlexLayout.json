{
	".flexLayout": {
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "rgba(255,255,255,0)",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"font": {},
		"flexProps": {
			"alignSelf": "AUTO",
			"direction": "INHERIT",
			"alignContent": "STRETCH",
			"alignItems": "STRETCH",
			"justifyContent": "FLEX_START",
			"flexWrap": "NOWRAP",
			"flexDirection": "COLUMN",
			"positionType": "RELATIVE"
		},
		"&-default": {
			"height": 200,
			"width": 200
		},
		"&-line": {
			"height": 1,
			"alpha": 0.1,
			"backgroundColor": "#1D1D26"
		},
		"&-margin": {
			"marginLeft": 20,
			"marginRight": 20
		}
	}
}
