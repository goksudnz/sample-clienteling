{
	".flexLayout": {
		"backgroundColor": "rgba(255,255,255,0)",
		"&-default": {
			"height": 200,
			"width": 200
		},
		"&-line": {
			"width": 0,
			"flexProps": {
				"flexGrow": 0
			},
			"height": 1,
			"alpha": 0.1,
			"backgroundColor": "#1D1D26"
		},
		"&-margin": {
			"marginLeft": 20,
			"marginRight": 20
		},
		"&-pgShoppingBagFlContinueButton": {
			"marginTop": 5,
			"marginBottom": 20,
			"height": 42,
			"width": null,
			"flexProps": {
				"alignItems": "CENTER",
				"flexDirection": "ROW",
				"justifyContent": "CENTER"
			},
			"+page:Screen.width > 451": {
				"flexProps": {
					"justifyContent": "FLEX_END"
				}
			}
		}
	}
}
