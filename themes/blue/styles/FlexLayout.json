{
	".flexLayout": {
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "rgba(255,255,255,0)",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"font": {},
		"flexProps": {
			"alignSelf": "AUTO",
			"direction": "INHERIT",
			"alignContent": "STRETCH",
			"alignItems": "STRETCH",
			"justifyContent": "FLEX_START",
			"flexWrap": "NOWRAP",
			"flexDirection": "COLUMN",
			"positionType": "RELATIVE"
		},
		"&-default": {
			"height": 200,
			"width": 200
		},
		"&-line": {
			"height": 1,
			"alpha": 0.1,
			"backgroundColor": "#1D1D26"
		},
		"&-margin": {
			"marginLeft": 20,
			"marginRight": 20
		},
		".signup": {
			"width": null,
			"height": null,
			"flexProps": {
				"flexGrow": 1,
				"flexDirection": "ROW",
				"alignItems": "FLEX_END"
			},
			".other": {
				"width": null,
				"height": null,
				"flexProps": {
					"flexGrow": 1,
					"flexDirection": "ROW",
					"alignItems": "AUTO"
				},
				"+page:Screen.width > 1000": {
					"flexProps": {
						"flexDirection": "COLUMN"
					}
				}
			},
			".main": {
				"flexProps": {
					"flexDirection": "COLUMN",
					"alignItems": "AUTO"
				},
				"+page:Screen.width > 1000": {
					"flexProps": {
						"flexDirection": "ROW"
					}
				},
				"&-phone": {
					"width": null,
					"height": null,
					"flexProps": {
						"flexGrow": 1
					},
					"+page:Screen.width > 375": {
						"flexProps": {
							"flexDirection": "ROW"
						}
					}
				}
			},
			".info": {
				"width": null,
				"height": null,
				"left": 0,
				"top": 0,
				"flexProps": {
					"flexGrow": 1,
					"flexDirection": "COLUMN",
					"alignItems": "AUTO"
				},
				"+page:Screen.width > 1000": {
					"paddingBottom": 100
				}
			}
		}
	}
}
